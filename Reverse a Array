//# Question - Write a program to reverse an array or string
//# Question Link - https://www.geeksforgeeks.org/write-a-program-to-reverse-an-array-or-string/
//# Solution - 


#include<bits/stdc++.h>
using namespace std;
int
main ()
{
  int n;
  cin >> n;
  int a[n];
  for (int i = 0; i < n; i++)
    {
      cin >> a[i];
    }
  int start = 0;
  int end = n - 1 ;
  while(start < end){
      int temp = a[start];
      a[start] = a[end];
      a[end] = temp;
      start++;
      end--;
  }
  for(auto value : a){
      cout<<value<<" ";
  }
}
